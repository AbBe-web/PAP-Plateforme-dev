  const foisAerobie = document.querySelector('input[name="fois_aerobie"]')?.value;

  if (intensite && foisAerobie) {
    ordonnance += `une ordonnance d‚ÄôAP a√©robie (${intensite === 'moderee' ? 'mod√©r√©e' : '√©lev√©e'}, ${foisAerobie} fois par semaine)`;
  }
}

if (document.getElementById("renforcement_musculaire")?.checked) {
